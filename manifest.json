{
    "manifest_version": 3,
    "name": "Script Load Tracker SEO",
    "version": "1.0",
    "description": "Track the scripts affecting the page load and performance. Made by Feres",
    "permissions": [
      "tabs",
      "scripting",
      "activeTab"
    ],
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icon16.png",
        "48": "icon48.png",
        "128": "icon128.png"
      }
    },
    "background": {
      "service_worker": "background.js"
    },
    "content_security_policy": {
      "extension_pages": "script-src 'self'; object-src 'self'"
    }
  }
  